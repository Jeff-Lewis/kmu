<p id="notice"><%= notice %></p>

<h1>Listing Service Documents</h1>

<table>
  <thead>
    <tr>
      <th>Service</th>
      <th>Name</th>
      <th>Description</th>
      <th>Document file name</th>
      <th>Document content type</th>
      <th>Document file size</th>
      <th>Document updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_documents.each do |service_document| %>
      <tr>
        <td><%= service_document.service_id %></td>
        <td><%= service_document.name %></td>
        <td><%= service_document.description %></td>
        <td><%= service_document.document_file_name %></td>
        <td><%= service_document.document_content_type %></td>
        <td><%= service_document.document_file_size %></td>
        <td><%= service_document.document_updated_at %></td>
        <td><%= link_to 'Show', service_document %></td>
        <td><%= link_to 'Edit', edit_service_document_path(service_document) %></td>
        <td><%= link_to 'Destroy', service_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service document', new_service_document_path %>
