<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Adress1</th>
      <th>Adress2</th>
      <th>Adress3</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Responsibility</th>
      <th>Costinfo1</th>
      <th>Costinfo2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.description %></td>
        <td><%= project.adress1 %></td>
        <td><%= project.adress2 %></td>
        <td><%= project.adress3 %></td>
        <td><%= project.phone1 %></td>
        <td><%= project.phone2 %></td>
        <td><%= project.responsibility %></td>
        <td><%= project.costinfo1 %></td>
        <td><%= project.costinfo2 %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
